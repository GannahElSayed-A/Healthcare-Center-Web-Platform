
<!DOCTYPE html>

<html lang="en">
 

<head>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
<link rel="preconnect" href="~/https://fonts.googleapis.com" >
<link rel="preconnect" href="~/https://fonts.gstatic.com" crossorigin >
<link href="~/https://fonts.googleapis.com/css2?family=Patrick+Hand&display=swap" rel="stylesheet">
   
<title>@ViewData["Title"] - GUI</title>
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<link rel="stylesheet" href="~/GUI.styles.css" asp-append-version="true" />
</head>
    <body style=" background: #045B73">
<header>
         
<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3" style=" background: #D9F6F3">
    
<div class="container">
                <div class="logo">
                    <img src="~/img/419358295_122104266260178950_6551667714851771567_n.jpg" alt="logo" class="img-1x rounded-circle mb-3" />
                </div>
    <a class="navbar-brand" asp-area="" asp-page="/Index">GUI</a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
            aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
           
    <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between" >
        <ul class="navbar-nav flex-grow-1">
            <li class="nav-item" >
                <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
            </li>
            <li class="nav-item" >
                <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Transactions</a>
            </li>
            <li class="nav-item" >
                <a class="nav-link text-dark" asp-area="" asp-page="/Schedule">Schedule</a>
            </li>
            <li class="nav-item" >
                <a class="nav-link text-dark" asp-area="" asp-page="/Feedbacks">Feedbacks</a>
            </li>
        </ul>
    </div>
</div>
</nav>
</header>
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2024 - GUI - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </div>
</footer>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>

@await RenderSectionAsync("Scripts", required: false)
</body>
       
</html>
