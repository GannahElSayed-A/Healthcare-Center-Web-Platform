@page
@model GUI.Pages.AddFModel
@{
}
<head>
    <meta charset="UTF-8">

    <title></title>
    <link href='http://fonts.googleapis.com/css?family=Questrial|Droid+Sans|Alice' rel='stylesheet' type='text/css'>
</head>
<body>
    <div class="container">
        <div class="row text-center">
            <div></div>

            <div id="content">
                <h1> Manage FeedBacks </h1>

                <form method="post" role="form">
                    <div class="form-group">
                        <label for="doc_name" class="iconic user"> Doctor <span class="required">*</span></label>
                        <input type="text" class="form-control" name="doc_name" id="doc_name" required="required" placeholder="Doctor's Name">
                    </div>
                    <div class="form-group">
                        <label for="p_name" class="iconic user"> Patient <span class="required">*</span></label>
                        <input type="text" class="form-control" name="p_name" id="doc_name" required="required" placeholder="Patient's Name">
                    </div>
                    <div class="form-group">
                        <label for="s_date" class="iconic mail-alt"> Session Date <span class="required">*</span></label>
                        <input type="text" class="form-control" name="s_date" id="s_date" placeholder="When was this session" required="required">
                    </div>
                    
                    <div class="form-group">
                        <label for="message" class="iconic comment"> Feedback </label>
                        <textarea name="message" id="message" class="form-control" rows="3" placeholder="Sum up the session in a few sentences" required="required"></textarea>
                    </div><br />
                    <span>
                        <input type="submit" value="Add" />
                        <input type="submit" value="Edit" />
                    </span>
                    
                   
                </form>
            </div>
        </div>

    </div>
</body>
